{
  "name": "fd-alpha",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev --host",
    "dev:ssl": "SECURE=on astro dev --host",
    "start": "astro dev --host --open",
    "check": "astro check",
    "build": "astro check && astro build",
    "serve": "netlify serve",
    "preview": "astro preview",
    "astro": "astro",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "test": "vitest --ui",
    "coverage": "vitest run --coverage",
    "sb:login": "storyblok login",
    "sb:logout": "storyblok logout",
    "sb:pull": "storyblok pull-components --space 285394 --path ./src/storyblok/ && storyblok-generate-ts source=./src/storyblok/components.285394.json target=./src/types/storyblok.d.ts",
    "sb:push": "node scripts/prompt-sb-push.js",
    "sb:pull-seperate": "storyblok pull-components --space 285394 --separate-files --file-name production --path ./src/storyblok/components/",
    "ssl": "local-ssl-proxy --source 4000 --target 4321 --cert localhost.pem --key localhost-key.pem",
    "ssl:netlify": "local-ssl-proxy --source 8000 --target 8888 --cert localhost.pem --key localhost-key.pem"
  },
  "dependencies": {
    "@astrojs/alpinejs": "^0.4.0",
    "@astrojs/check": "^0.5.10",
    "@astrojs/tailwind": "^5.1.0",
    "@fontsource-variable/inconsolata": "^5.0.17",
    "@fontsource-variable/sofia-sans-extra-condensed": "^5.0.13",
    "@headlessui/react": "^2.1.0",
    "@heroicons/react": "^2.1.4",
    "@iframe-resizer/child": "^5.1.4",
    "@loomhq/loom-embed": "^1.7.0",
    "@storyblok/astro": "^4.0.4",
    "@tailwindcss/typography": "^0.5.12",
    "@types/alpinejs": "^3.13.10",
    "alpinejs": "^3.13.8",
    "astro": "^4.5.12",
    "clsx": "^2.1.0",
    "format-duration": "^3.0.2",
    "lodash": "^4.17.21",
    "srt-parser-2": "^1.2.3",
    "storyblok-rich-text-astro-renderer": "^2.2.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.3"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "storyblok": "^3.30.0",
    "storyblok-generate-ts": "^2.1.0"
  }
}
